    "gatsby-plugin-offline": "^4.4.0",
    "gatsby-plugin-react-helmet": "^4.4.0",
    "gatsby-plugin-sharp": "^3.4.2",
    "gatsby-plugin-sitemap": "^4.0.0",
    "gatsby-plugin-typescript": "^3.4.0",
    "gatsby-remark-images": "^5.1.0",
    "gatsby-source-filesystem": "^3.4.0",
    "gatsby-transformer-remark": "^4.1.0",
    "gatsby-transformer-sharp": "^3.4.0",
    "normalize.css": "^8.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0"
  },
  "description": "ITFarrier",
  "devDependencies": {
    "@types/graphql": "^14.5.0",
    "@types/node": "^15.0.2",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.3",
    "@types/react-helmet": "^6.1.1",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-json-es": "^1.5.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sort-destructure-keys": "^1.3.5",
    "gh-pages": "^3.1.0",
    "npm-check-updates": "^11.5.10",
    "prettier": "^2.3.0",
    "typescript": "^4.2.4"
  },
  "keywords": [
    "IT Farrier",
    "IT",
    "farrier",
    "itfarrier"
  ],
  "name": "itfarrier",
  "private": true,
  "scripts": {
    "build": "gatsby build --verbose",
    "clean": "rm -rf .cache public",
    "deploy": "yarn clean && git push origin source && yarn build && cp CNAME public/ && gh-pages -b master -d public",
    "deploy-test": "yarn clean && git push test && yarn build && cp CNAME-TEST public/CNAME && gh-pages -b master -d public -o test",
    "eslint": "eslint . --ext .js,.jsx",
    "eslint-auto-fix-the-whole-project": "eslint . -c ./.eslintrc.autoFixes.js --debug --fix --no-eslintrc",
    "prettier-the-whole-project": "prettier --loglevel debug --write '**/*.{css,js,json,jsx,markdown,md,scss,ts,tsx}'",
    "setup": "rm -rf .cache node_modules public && yarn --verbose",
    "start": "yarn clean && gatsby develop -p 8000 --verbose",
    "start-prod": "yarn clean && yarn build && gatsby serve -p 8000 --verbose",
    "upgrade-all-deps": "ncu -l debug --timeout 999999999 -u && yarn && yarn up -C",
    "upgrade-dev-deps": "ncu --dep dev -l debug -u && yarn --verbose",
    "upgrade-prod-deps": "ncu --dep prod -l debug -u && yarn --verbose"
  },
  "version": "3.0.0"
}
