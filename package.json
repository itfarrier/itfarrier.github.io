  "dependencies": {
    "gatsby": "^3.11.1",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-i18n": "^1.0.1",
    "gatsby-plugin-image": "^1.11.0",
    "gatsby-plugin-manifest": "^3.11.0",
    "gatsby-plugin-offline": "^4.11.0",
    "gatsby-plugin-react-helmet": "^4.11.0",
    "gatsby-plugin-sharp": "^3.11.0",
    "gatsby-plugin-sitemap": "^4.7.0",
    "gatsby-plugin-typegen": "^2.2.4",
    "gatsby-plugin-typescript": "^3.11.0",
    "gatsby-remark-images": "^5.8.0",
    "gatsby-source-filesystem": "^3.11.0",
    "gatsby-transformer-remark": "^4.8.0",
    "gatsby-transformer-sharp": "^3.11.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "sanitize.css": "^12.0.1"
  },
  "description": "ITFarrier",
  "devDependencies": {
    "@types/graphql": "^14.5.0",
    "@types/inquirer": "^7.3.3",
    "@types/node": "^16.6.1",
    "@types/react": "^17.0.18",
    "@types/react-dom": "^17.0.9",
    "@types/react-helmet": "^6.1.2",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-graphql": "^4.0.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-json-es": "^1.5.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sort-destructure-keys": "^1.3.5",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.1",
    "inquirer": "^8.1.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "ts-graphql-plugin": "^2.1.4",
    "typescript": "^4.3.5"
  },
  "keywords": [
    "IT Farrier",
    "IT",
    "farrier",
    "itfarrier"
  ],
  "name": "itfarrier",
  "private": true,
  "scripts": {
    "build": "gatsby build --verbose",
    "check-useless-dependencies": "npx depcheck",
    "clean": "find . -name '*.log' -type f -delete && rm -rf .cache node_modules public",
    "clean-build": "rm -rf .cache public",
    "clean-lockfile": "rm -rf package-lock.json yarn.lock",
    "create-new-post": "npx ts-node -P ./tsconfig.cli.json ./src/utilities/cli.ts",
    "deploy": "npm run clean-build && git push origin source && npm run build && cp CNAME public/ && gh-pages -b master -d public",
    "deploy-test": "npm run clean && git push test && npm run build && cp CNAME-TEST public/CNAME && gh-pages -b master -d public -o test",
    "eslint": "eslint . --ext .js,.jsx",
    "eslint-auto-fix-the-whole-project": "eslint . -c ./.eslintrc.autoFixes.js --debug --fix --no-eslintrc",
    "prepare": "husky install",
    "prettier-the-whole-project": "prettier --loglevel debug --write '**/*.{css,js,json,jsx,markdown,md,scss,ts,tsx}'",
    "reset": "npm run clean && npm run setup",
    "reset-full": "npm run clean && npm run clean-lockfile && npm run setup",
    "setup": "npm i",
    "start": "npm run clean-build && gatsby develop -p 8000 --verbose",
    "start-prod": "npm run clean-build && npm run build && gatsby serve -p 8000 --verbose",
    "upgrade-all-deps": "npx npm-check-updates --concurrency 999999999 --deep -l silly --timeout 999999999 -u && npm i"
  },
  "version": "3.0.0"
}
