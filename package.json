  },
  "scripts": {
    "build": "gatsby build --verbose",
    "clean": "rm -rf ./.cache/* && rm -rf ./public/*",
    "deploy": "yarn run clean && git push origin source && yarn run build && cp CNAME public/ && gh-pages -b master -d public",
    "deployTest": "yarn run clean && git push test && yarn run build && cp CNAME-TEST public/CNAME && gh-pages -b master -d public -o test",
    "setup": "rm -rf .cache node_modules public && yarn",
    "start": "yarn run clean && gatsby develop -p 8000 --verbose",
    "startProd": "yarn run clean && yarn run build && gatsby serve -p 8000 --verbose",
    "tslint": "tslint -p ./",
    "tslintFixImportSortOrder": "tslint -c ./tslintSortOrder.json --fix -p ./",
    "upgradeAllDepsToLatest": "ncu -l debug -u && yarn",
    "upgradeDevDepsToLatest": "ncu --dep dev -l debug -u && yarn",
    "upgradeProdDepsToLatest": "ncu --dep prod -l debug -u && yarn"
  },
  "version": "2.0.0"
}
