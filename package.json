  "dependencies": {
    "gatsby": "^3.7.1",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-i18n": "^1.0.1",
    "gatsby-plugin-image": "^1.7.1",
    "gatsby-plugin-manifest": "^3.7.1",
    "gatsby-plugin-offline": "^4.7.1",
    "gatsby-plugin-react-helmet": "^4.7.1",
    "gatsby-plugin-sharp": "^3.7.1",
    "gatsby-plugin-sitemap": "^4.3.1",
    "gatsby-plugin-typescript": "^3.7.1",
    "gatsby-remark-images": "^5.4.1",
    "gatsby-source-filesystem": "^3.7.1",
    "gatsby-transformer-remark": "^4.4.1",
    "gatsby-transformer-sharp": "^3.7.1",
    "normalize.css": "^8.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0"
  },
  "description": "ITFarrier",
  "devDependencies": {
    "@types/graphql": "^14.5.0",
    "@types/node": "^15.12.2",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "@types/react-helmet": "^6.1.1",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-json-es": "^1.5.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sort-destructure-keys": "^1.3.5",
    "gh-pages": "^3.2.0",
    "npm-check-updates": "^11.6.0",
    "prettier": "^2.3.1",
    "typescript": "^4.3.2"
  },
  "keywords": [
    "IT Farrier",
    "IT",
    "farrier",
    "itfarrier"
  ],
  "name": "itfarrier",
  "private": true,
  "scripts": {
    "build": "gatsby build --verbose",
    "clean": "rm -rf .cache public",
    "deploy": "npm run clean && git push origin source && npm run build && cp CNAME public/ && gh-pages -b master -d public",
    "deploy-test": "npm run clean && git push test && npm run build && cp CNAME-TEST public/CNAME && gh-pages -b master -d public -o test",
    "eslint": "eslint . --ext .js,.jsx",
    "eslint-auto-fix-the-whole-project": "eslint . -c ./.eslintrc.autoFixes.js --debug --fix --no-eslintrc",
    "prettier-the-whole-project": "prettier --loglevel debug --write '**/*.{css,js,json,jsx,markdown,md,scss,ts,tsx}'",
    "setup": "rm -rf .cache node_modules public && npm i",
    "start": "npm run clean && gatsby develop -p 8000 --verbose",
    "start-prod": "npm run clean && npm run build && gatsby serve -p 8000 --verbose",
    "upgrade-all-deps": "ncu -l debug --timeout 999999999 -u && npm i",
    "upgrade-dev-deps": "ncu --dep dev -l debug -u && npm i",
    "upgrade-prod-deps": "ncu --dep prod -l debug -u && npm i"
  },
  "version": "3.0.0"
}
